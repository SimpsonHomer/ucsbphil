                                        <tr>
                                          <td align="left" valign="top"><table width="938" border="0" cellspacing="0" cellpadding="0">
                                              <tr height="25">
                                                <td align="left" valign="middle" bgcolor="#cccccc" height="25"><table border="0" cellspacing="0" cellpadding="0">
                                                    <tr>
														<td class="sub-navigation" align="left" valign="middle">
							<!-- Course Number-->
							Philosophy 112
														</td>
														<td class="sub-navigation" align="center" valign="middle" width="15"><div align="center"> |</div></td>
														<td class="sub-navigation" align="left" valign="middle">
							<!-- Course Title-->
							Philosophy of Religion
														</td>
														<td class="sub-navigation" align="center" valign="middle" width="15"><div align="center"> |</div></td>
                                                      	<td class="sub-navigation" align="left" valign="middle">
							<!-- Location-->                                                        
							Session A | SH 1430 | TWR 2:00 PM-3:25 PM + Discussion
														</td>
                                                    </tr>
                                                </table></td>
                                              	</tr>
